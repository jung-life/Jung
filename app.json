{
  "expo": {
    "name": "jung",
    "slug": "jung",
    "version": "9.0.0",
    "orientation": "portrait",
    "icon": "./assets/jung-app-logo-1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/jung-app-logo-1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.name.jung",
      "buildNumber": "9",
      "icon": "./assets/jung-app-logo-1024.png",
      "associatedDomains": [
        "applinks:auth.expo.io"
      ],
      "infoPlist": {
        "CFBundleIconName": "AppIcon",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save files to your photo library.",
        "NSLocationWhenInUseUsageDescription": "Jung needs your location to provide personalized mental health support and track your mood patterns in different environments.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Jung needs your location to provide personalized mental health support and track your mood patterns in different environments, even when the app is in the background.",
        "NSLocationAlwaysUsageDescription": "Jung needs your location to provide personalized mental health support and track your mood patterns in different environments, even when the app is in the background.",
        "NSMicrophoneUsageDescription": "This app needs microphone access for voice messages and audio recording features.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "google",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.478933387478-p4lgnk2bet20h01olhh9kj033d3l1i2h"
            ]
          }
        ]
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/jung-app-logo-1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.jungai",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "auth.expo.io"
            },
            {
              "scheme": "jung"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/jung-app-logo-1024.png",
      "bundler": "metro"
    },
    "scheme": "jung",
    "extra": {
      "eas": {
        "projectId": "c4f0be2f-a422-489b-acda-4d2be4769a36"
      }
    },
    "owner": "infinitydata.ai",
    "plugins": [
      "expo-secure-store",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin"
    ]
  }
}
