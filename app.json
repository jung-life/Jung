{
  "expo": {
    "name": "Jung AI",
    "slug": "jung-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/JungAppLogoAlpha.webp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/JungAppLogoAlpha.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.jungai",
      "associatedDomains": [
        "applinks:yourapp.page.link"
      ],
      "config": {
        "googleSignIn": {
          "reservedClientId": "your-google-client-id"
        }
      },
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save files to your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/JungAppLogoAlpha.webp",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.jungai",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.yourapp.page.link"
            },
            {
              "scheme": "yourapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/JungAppLogoAlpha.png"
    },
    "scheme": "yourapp",
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "owner": "infinitydata.ai",
    "plugins": [
      "expo-secure-store"
    ]
  }
}
