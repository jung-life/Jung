{
  "expo": {
    "name": "jung",
    "slug": "jung",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo/jung-app-log.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo/jung-app-log.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.jungai",
      "associatedDomains": [
        "applinks:auth.expo.io"
      ],
      "config": {
        "googleSignIn": {
          "reservedClientId": "your-google-client-id"
        }
      },
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save files to your photo library."
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo/jung-app-log.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.jungai",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "auth.expo.io"
            },
            {
              "scheme": "jung"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/logo/jung-app-log.png",
      "bundler": "metro"
    },
    "scheme": "jung",
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "owner": "infinitydata.ai",
    "plugins": [
      "expo-secure-store",
      "@react-native-google-signin/google-signin"
    ]
  }
}
