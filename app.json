{
  "expo": {
    "name": "jung",
    "slug": "jung",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo/jung-app-icon-1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo/jung-app-log.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.name.jung",
      "buildNumber": "7489",
      "associatedDomains": [
        "applinks:auth.expo.io"
      ],
      "config": {
        "googleSignIn": {
          "reservedClientId": "your-google-client-id"
        }
      },
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save files to your photo library.",
        "NSLocationWhenInUseUsageDescription": "Jung needs your location to provide personalized mental health support and track your mood patterns in different environments.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Jung needs your location to provide personalized mental health support and track your mood patterns in different environments, even when the app is in the background.",
        "NSLocationAlwaysUsageDescription": "Jung needs your location to provide personalized mental health support and track your mood patterns in different environments, even when the app is in the background.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSAllowsArbitraryLoadsInWebContent": false
        },
        "UIRequiredDeviceCapabilities": [
          "arm64"
        ],
        "LSRequiresIPhoneOS": true
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "icon": "./src/assets/logo/jung-app-icon-1024.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo/jung-app-icon-1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.jungai",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "auth.expo.io"
            },
            {
              "scheme": "jung"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/logo/jung-app-icon-1024.png",
      "bundler": "metro"
    },
    "scheme": "jung",
    "extra": {
      "eas": {
        "projectId": "c4f0be2f-a422-489b-acda-4d2be4769a36"
      }
    },
    "owner": "infinitydata.ai",
    "plugins": [
      "expo-secure-store",
      "@react-native-google-signin/google-signin"
    ]
  }
}