{
  "cli": {
    "version": ">= 7.3.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "capabilities": {
          "SignInWithApple": true
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "capabilities": {
          "SignInWithApple": true
        }
      }
    },
    "production": {
      "env": {
        "NODE_ENV": "production"
      },
      "ios": {
        "buildConfiguration": "Release",
        "capabilities": {
          "SignInWithApple": true
        }
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "env": {
        "NODE_ENV": "development",
        "EXPO_PUBLIC_ENVIRONMENT": "simulator"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
